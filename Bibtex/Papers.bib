@Article{Adolfsson2005,
  Title                    = {Identification of Flt3+ Lympho-Myeloid Stem Cells Lacking Erythro-Megakaryocytic Potential: A Revised Road Map for Adult Blood Lineage Commitment },
  Author                   = {Jorgen Adolfsson and Robert Mansson and Natalija Buza-Vidas and Anne Hultquist and Karina Liuba and Christina T. Jensen and David Bryder and Liping Yang and Ole-Johan Borge and Lina A.M. Thoren and Kristina Anderson and Ewa Sitnicka and Yutaka Sasaki and Mikael Sigvardsson and Sten Eirik W. Jacobsen},
