  Doi                      = {http://dx.doi.org/10.1016/j.cell.2005.02.013},
  ISSN                     = {0092-8674},
  Owner                    = {Helen},
  Timestamp                = {2015.08.10},
  Url                      = {http://www.sciencedirect.com/science/article/pii/S0092867405001583}
}

